{
  "name": "3rdhand.info",
  "version": "0.0.0",
  "description": "Reidar Djupvik's personal website",
  "main": "index.html",
  "bin": {
    "drop-db": "bin/drop-db.js",
    "insert-ddoc": "bin/insert-ddoc.js"
  },
  "scripts": {
    "build": "npm run svgo && NODE_ENV=production webpack",
    "clean": "rm -r dist/*",
    "drop-db": "drop-db",
    "insert-ddoc": "insert-ddoc",
    "lint": "coffeelint lib/ src/",
    "test": "mocha --compilers coffee:coffee-script/register",
    "posttest": "npm run lint",
    "start": "coffee server.coffee",
    "svgo": "svgo --config=./svgo.yaml -f ./src/svg -o ./dist/svg",
    "server": "webpack-dev-server --inline --hot"
  },
